{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "types": [
      "vite/client",
      "@cloudflare/workers-types/2023-07-01"
    ],
  },
  "include": [
    "src",
    "src-backend",
    "teenybase.ts",
  ]
}
